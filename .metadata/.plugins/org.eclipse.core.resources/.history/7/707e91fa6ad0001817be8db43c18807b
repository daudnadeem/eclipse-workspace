package com.qa.Library;

public class Books extends LibraryItems {

	private String name;
	private String type;

	public Books(String name, String type, int itemID) {
		super(itemID);
		name = this.name;
		type = this.type;
		itemId = LibraryItems.setItemID(itemID);
		
		
	}

	public String getName() {
		return name;
	}

	public int getItemID() {
		return LibraryItems.itemID;
	}

	public String getType() {
		return type;
	}

}
