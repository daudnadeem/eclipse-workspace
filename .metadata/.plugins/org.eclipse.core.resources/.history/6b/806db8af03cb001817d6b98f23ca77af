import java.util.ArrayList;

public class Runner {
	
	public static void main(String [] args) {
		numToArray();
	}
	
	public static int[] numToArray() {
		int [] myArray  = new int [100]; 
		for(int i=0; i<myArray.length;i++) {
			myArray[i] = i+1;
		}
		return myArray;
	}
	
	public static void numToList() {
		List myList = new ArrayList();
	}

}
