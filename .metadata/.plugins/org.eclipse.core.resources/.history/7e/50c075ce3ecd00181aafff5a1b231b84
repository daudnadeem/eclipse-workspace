package com.qa.TestDriven;

public class ArrayUtils {
	

	public static int findHighest(int[] numbers) {
		
		int[] array = { 2, 10, 9, 20 };
		int lowestValue = numbers[0];
		int curValue = 0;
		for (int i=0; i<numbers.length; i++) {
			if (lowestValue > array[i]) {
				curValue = lowestValue;
			}
		}
		return curValue;

	}
}
